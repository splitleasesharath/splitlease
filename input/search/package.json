{
  "name": "split-lease-search",
  "version": "1.0.0",
  "description": "Split Lease property search page with React islands architecture",
  "type": "module",
  "scripts": {
    "build": "npm run build:components && npm run build:cloudflare",
    "build:cloudflare": "node build-cloudflare.js",
    "build:components": "vite build",
    "dev": "npm run dev:components",
    "dev:components": "vite build --watch",
    "lint": "eslint 'js/**/*.js' 'components/**/*.{js,jsx,ts,tsx}' --max-warnings 0",
    "lint:fix": "npm run lint -- --fix",
    "format": "prettier --write '**/*.{js,jsx,ts,tsx,json,css,md}'",
    "format:check": "prettier --check '**/*.{js,jsx,ts,tsx,json,css,md}'",
    "prepare": "node ../../scripts/setup-git-merge-driver.sh 2>/dev/null || true",
    "serve": "python -m http.server 8000",
    "start": "python server.py",
    "test": "npm run lint && npm run format:check && npm run type-check",
    "type-check": "tsc --noEmit"
  },
  "dependencies": {
    "framer-motion": "^10.16.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "styled-components": "^6.1.0"
  },
  "devDependencies": {
    "@types/react": "^18.2.0",
    "@types/react-dom": "^18.2.0",
    "@vitejs/plugin-react": "^4.2.0",
    "eslint": "^8.57.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-react": "^7.33.0",
    "eslint-plugin-react-hooks": "^4.6.0",
    "playwright": "^1.55.1",
    "prettier": "^3.2.0",
    "typescript": "^5.0.0",
    "vite": "^5.0.0"
  },
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=9.0.0"
  },
  "author": "Split Lease",
  "license": "MIT",
  "keywords": [
    "split-lease",
    "property-search",
    "react",
    "supabase",
    "real-estate"
  ]
}
