{
  "name": "split-lease",
  "version": "1.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite --port 8000",
    "generate-routes": "node scripts/generate-redirects.js",
    "prebuild": "bun run generate-routes",
    "build": "vite build",
    "build:dev": "vite build --mode development",
    "preview": "vite preview --port 8000",
    "deploy": "wrangler pages deploy dist --project-name splitlease",
    "deploy:dev": "bun run build:dev && wrangler pages deploy dist --project-name splitlease --branch development",
    "lint": "eslint src/",
    "lint:fix": "eslint src/ --fix",
    "lint:check": "eslint src/ --max-warnings 0"
  },
  "dependencies": {
    "@hookform/resolvers": "^5.2.2",
    "@react-google-maps/api": "^2.20.7",
    "@supabase/supabase-js": "^2.38.0",
    "clsx": "^2.1.1",
    "date-fns": "^4.1.0",
    "date-fns-tz": "^3.2.0",
    "framer-motion": "^12.23.24",
    "gsap": "^3.14.2",
    "lottie-react": "^2.4.1",
    "lucide-react": "^0.553.0",
    "motion-utils": "^12.23.6",
    "qrcode.react": "^4.2.0",
    "react": "^18.2.0",
    "react-datepicker": "^8.9.0",
    "react-dom": "^18.2.0",
    "react-hook-form": "^7.66.1",
    "styled-components": "^6.1.19",
    "tailwind-merge": "^3.4.0",
    "zod": "^4.1.12"
  },
  "devDependencies": {
    "@vitejs/plugin-react": "^4.2.0",
    "autoprefixer": "^10.4.23",
    "dotenv": "^17.2.3",
    "esbuild": "^0.27.0",
    "eslint": "^9.39.2",
    "eslint-plugin-react": "^7.37.5",
    "eslint-plugin-react-hooks": "^7.0.1",
    "globals": "^17.0.0",
    "postcss": "^8.5.6",
    "supabase": "^2.58.5",
    "tailwindcss": "3",
    "typescript": "^5.9.3",
    "vite": "^5.0.0"
  }
}
